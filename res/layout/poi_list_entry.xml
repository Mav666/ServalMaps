<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/smapsLayoutmatch"
    android:orientation="vertical" 
    android:id="@+id/poi_list_ui_entry">

    <TextView
        android:id="@+id/poi_list_ui_entry_title"
        style="@style/smapsLabel" />
    
     <TableLayout
        style="@style/smapsLayout" >

        <TableRow
            android:id="@+id/tableRow1"
            style="@style/smapsWrap" >
            
            <TextView
		        android:id="@+id/poi_list_ui_entry_age_lbl"
		        style="@style/smapsLabel" 
		        android:text="@string/poi_info_ui_lbl_age"/>
		    
		    <TextView
		        android:id="@+id/poi_list_ui_entry_age"
		        style="@style/smapsTextwrapped" />
		    
        </TableRow>
        
        <TableRow
            android:id="@+id/tableRow2"
            style="@style/smapsWrap" >
		    
            <TextView
		        android:id="@+id/poi_list_ui_lbl_distance"
		        style="@style/smapsLabel" 
		        android:text="@string/poi_info_ui_lbl_distance" />
		
		    <TextView
		        android:id="@+id/poi_list_ui_txt_distance"
		        style="@style/smapsTextwrapped" 
		        android:text="" />
    
        </TableRow>
        
        <TableRow
            android:id="@+id/tableRow3"
            style="@style/smapsWrap" >
		    
            <TextView
		        android:id="@+id/poi_list_ui_lbl_tags"
		        style="@style/smapsLabel" 
		        android:text="@string/poi_info_ui_lbl_tags" />
		
		    <TextView
		        android:id="@+id/poi_list_ui_txt_tags"
		        style="@style/smapsTextwrapped" 
		        android:text="" />
    
        </TableRow>
        
   </TableLayout>
   
   <View 
      android:id="@+id/Separator"
	  style="@style/smapsLayout"
	  android:background="?android:attr/listDivider" />

</LinearLayout>